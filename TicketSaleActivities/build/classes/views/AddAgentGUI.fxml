<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="addAgentAnchorPane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane alignment="TOP_RIGHT" prefHeight="400.0" prefWidth="400.0">
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="0.0" prefHeight="0.0" valignment="TOP" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="cancelBtn" alignment="CENTER" mnemonicParsing="false" onAction="#cancelAddingAgentAction" prefWidth="150.0" text="Cancel" textAlignment="CENTER" GridPane.rowIndex="4" />
            <Button fx:id="addAgentBtn" alignment="CENTER" mnemonicParsing="false" onAction="#addAgentAction" prefWidth="150.0" text="Add Agent" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1">
               <children>
                  <TextField fx:id="agentName" maxWidth="150.0" prefWidth="150.0">
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin></TextField>
                  <Label fx:id="nameErrorLabel" alignment="TOP_CENTER" contentDisplay="CENTER" minHeight="50.0" prefWidth="150.0" textAlignment="CENTER" textFill="RED" wrapText="true">
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                     <font>
                        <Font name="System Bold" size="10.0" />
                     </font>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <padding>
                        <Insets top="3.0" />
                     </padding></Label>
               </children>
            </VBox>
            <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <TextField fx:id="agentLocation" maxWidth="150.0" prefWidth="150.0" />
                  <Label fx:id="locationErrorLabel" alignment="TOP_CENTER" contentDisplay="CENTER" minHeight="50.0" prefWidth="150.0" textFill="RED" wrapText="true">
                     <font>
                        <Font name="System Bold" size="10.0" />
                     </font>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <padding>
                        <Insets top="3.0" />
                     </padding>
                  </Label>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <GridPane.margin>
                  <Insets left="10.0" />
               </GridPane.margin>
            </VBox>
            <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <children>
                  <TextField fx:id="agentPhoneNo" maxWidth="150.0" prefWidth="150.0" />
                  <Label fx:id="phoneErrorLabel" alignment="TOP_CENTER" contentDisplay="CENTER" minHeight="50.0" prefWidth="150.0" textAlignment="CENTER" textFill="RED" wrapText="true">
                     <font>
                        <Font name="System Bold" size="10.0" />
                     </font>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <padding>
                        <Insets top="3.0" />
                     </padding>
                  </Label>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <GridPane.margin>
                  <Insets left="10.0" />
               </GridPane.margin>
            </VBox>
            <VBox alignment="CENTER_RIGHT" prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="3">
               <children>
                  <Label text="Active" />
               </children>
               <GridPane.margin>
                  <Insets right="10.0" top="-50.0" />
               </GridPane.margin>
            </VBox>
            <VBox alignment="CENTER_RIGHT" prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="2">
               <children>
                  <Label text="Agent Phone Number" />
               </children>
               <GridPane.margin>
                  <Insets right="10.0" top="-50.0" />
               </GridPane.margin>
            </VBox>
            <VBox alignment="CENTER_RIGHT" prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="1">
               <children>
                  <Label text="Agent Location" />
               </children>
               <GridPane.margin>
                  <Insets right="10.0" top="-50.0" />
               </GridPane.margin>
            </VBox>
            <VBox alignment="CENTER_RIGHT" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label text="Agent Name" />
               </children>
               <GridPane.margin>
                  <Insets right="10.0" top="-50.0" />
               </GridPane.margin>
            </VBox>
            <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <children>
                  <CheckBox fx:id="activeAgentCheckbox" mnemonicParsing="false" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <GridPane.margin>
                  <Insets left="10.0" top="-50.0" />
               </GridPane.margin>
            </VBox>
         </children>
      </GridPane>
   </children>
</AnchorPane>
